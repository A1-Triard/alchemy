SCTX
    Begin A1V4_ADel6_Levitate_sc
    
    if ( player->GetItemCount "A1V4_L30_levitation" == 1 )
    	player->RemoveItem "A1V4_L30_levitation", 1
    elseif ( player->GetItemCount "A1V4_L60_levitation" == 1 )
    	player->RemoveItem "A1V4_L60_levitation", 1
    endif
    
    StopScript A1V4_ADel6_Levitate_sc
    StartScript A1V4_ADel7_Tele_sc
    
    End
