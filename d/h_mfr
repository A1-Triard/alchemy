    	set doOnce to 1
    	set cx to A1V6_RetortX
    	set cy to A1V6_RetortY
    	set cz to A1V6_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

SCPT
SCHD A1V6_Retort_3_sc 1 0 3 224 16
SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V6_Retort_3_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V6_ApparatusDisable_3 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V6_RetortX
    	set cy to A1V6_RetortY
    	set cz to A1V6_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

SCPT
SCHD A1V6_StartAlchemy 0 0 0 36 0
SCDT DAEGcGxheWVy1BAUQTFWNl9BcHBhcmF0dXNJdGVtXzEBAAEB
SCTX
    Begin A1V6_StartAlchemy
    
    player->AddItem A1V6_ApparatusItem_1, 1
    
    End
    

