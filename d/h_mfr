    elseif ( player->GetItemCount apparatus_a_calcinator_01 > 0 )
    	player->RemoveItem apparatus_a_calcinator_01, 1
    	AddItem apparatus_a_calcinator_01, 1
    	PlaceAtPC A1V6_apparatus_a_calcinator_e3, 1, 50, 0
    endif
    
    if ( player->GetItemCount apparatus_g_retort_01 > 0 )
    	player->RemoveItem apparatus_g_retort_01, 1
    	AddItem apparatus_g_retort_01, 1
    	PlaceAtPC A1V6_apparatus_g_retort_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_m_retort_01 > 0 )
    	player->RemoveItem apparatus_m_retort_01, 1
    	AddItem apparatus_m_retort_01, 1
    	PlaceAtPC A1V6_apparatus_m_retort_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_j_retort_01 > 0 )
    	player->RemoveItem apparatus_j_retort_01, 1
    	AddItem apparatus_j_retort_01, 1
    	PlaceAtPC A1V6_apparatus_j_retort_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_a_retort_01 > 0 )
    	player->RemoveItem apparatus_a_retort_01, 1
    	AddItem apparatus_a_retort_01, 1
    	PlaceAtPC A1V6_apparatus_a_retort_e3, 1, 50, 0
    endif
    
    if ( player->GetItemCount apparatus_g_mortar_01 > 0 )
    	player->RemoveItem apparatus_g_mortar_01, 1
    	AddItem apparatus_g_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_g_mortar_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_m_mortar_01 > 0 )
    	player->RemoveItem apparatus_m_mortar_01, 1
    	AddItem apparatus_m_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_m_mortar_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_j_mortar_01 > 0 )
    	player->RemoveItem apparatus_j_mortar_01, 1
    	AddItem apparatus_j_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_j_mortar_e3, 1, 50, 0
    elseif ( player->GetItemCount apparatus_a_mortar_01 > 0 )
    	player->RemoveItem apparatus_a_mortar_01, 1
    	AddItem apparatus_a_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_a_mortar_e3, 1, 50, 0
    endif
    
