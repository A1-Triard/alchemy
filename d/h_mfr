SCPT
SCHD A1V6_ADelPlus 2 7 1 570 85
SCVR doRemove;temp;pcRef;invRef;invItem;invCount;invRefType;tempLong;invNextRef;tempFloat;;
SCDT 
SCTX
    Begin A1V6_ADelPlus
    
    short doRemove
    long pcRef
    long invRef
    long invItem
    long invCount
    long invRefType
    long tempLong
    float tempFloat
    long invNextRef
    short temp
    
    if ( doRemove == 1 )
    	set doRemove to 0
    	MessageBox "Установка деактивирована"
    	EnablePlayerControls
    	StopScript A1V6_ADelPlus
    	return
    endif
    
    setx pcRef to xGetRef "player"
    whilex ( pcRef )
    	setx invItem,invCount,invRefType,tempLong,tempFloat,tempLong,invNextRef to pcRef->xContentList invRef
    	set invRef to invNextRef
    	ifx ( invItem )
    		if ( invRefType == 1263488834 )
    			set temp to 1
    		else
    			set temp to 0
    		endif
    		ifx ( temp )
    			setx tempLong to xStringCompare invItem "A1V6_AlchemyPlus_"
    			if ( tempLong <= 0 )
    				set temp to 0
    			endif
    		endif
    		ifx ( temp )
    			setx tempLong to xStringCompare invItem "A1V6_AlchemyPlus_a"
    			if ( tempLong >= 0 )
    				set temp to 0
    			endif
    		endif
    		ifx ( temp )
    			pcRef->xRemoveItem invItem invCount
    			set invRef to 0
    			set pcRef to 0
    		endif
    	endif
    	if ( invNextRef == 0 )
    		set pcRef to 0
    		set doRemove to 1
    	endif
    endwhile
    
    End

SCPT
SCHD A1V6_AlchemyActivator_sc 2 0 0 56 22
SCVR PCSkipEquip;OnPCEquip;;
SCDT BQFzAQACIDEGAQIJIHMCACA9PSAxBQFzAgACIDAbEBRBMVY2X0FsY2hlbXlDaGVja19zYwkBAQE=
SCTX
    Begin A1V6_AlchemyActivator_sc
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( OnPCEquip == 1 )
    	set OnPCEquip to 0
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    End

