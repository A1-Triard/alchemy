SCTX
    Begin A1V4_ApparatusDeactivate_sc
    
    if ( player->GetItemCount A1V4_AlchemyActivator > 0 )
    	player->RemoveItem A1V4_AlchemyActivator, 1
    endif
    
    StopScript A1V4_ApparatusDeactivate_sc
    StartScript A1V4_ADel1_RestoreHealth_sc
    
    End

