    	set A1V6_RetortY to retortY
    	set A1V6_ApparatusZ to cz
    	StartScript A1V6_ApparatusSetUp_3
    	return
    endif
    
    if ( OnActivate == 1 )
    	DisablePlayerControls
    	set invRef to 0
    	set doRemove to 1
    	return
    endif
    
    End

