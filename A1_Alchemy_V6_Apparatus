    set sum to ( sum + in4 )
    set sum to ( sum + in5 )
    set sum to ( sum + in6 )
    set sum to ( sum + in7 )
    set sum to ( sum + in8 )
    set temp to ( in1 + in6 )
    if ( temp > 1 )
    	set sum to 0
    endif
    set temp to ( in2 + in3 )
    if ( temp > 1 )
    	set sum to 0
    endif
    set temp to ( in5 + in6 )
    if ( temp > 1 )
    	set sum to 0
    endif
    set temp to ( in7 + in8 )
    if ( temp > 1 )
    	set sum to 0
    endif
    if ( sum > 1 )
    	if ( in1 > 0 )
    		player->RemoveItem "ingred_black_lichen_01", 1
    	endif
    	if ( in2 > 0 )
    		player->RemoveItem "ingred_ghoul_heart_01", 1
    	endif
    	if ( in3 > 0 )
    		player->RemoveItem "ingred_rat_meat_01", 1
    	endif
