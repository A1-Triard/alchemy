    	set in3 to 1
    endif
    if ( player->GetItemCount "ingred_treated_bittergreen_uniq" > 0 )
    	set in4 to 1
    endif
    if ( player->GetItemCount "ingred_wolfsbane_01" > 0 )
    	set in5 to 1
    endif
    
    set sum30 to 0
    set sum30 to ( sum30 + in1 )
    set sum30 to ( sum30 + in2 )
    set sum30 to ( sum30 + in3 )
    set sum30 to ( sum30 + in5 )
    set sum60 to sum30
    set sum60 to ( sum60 + in4 )
    set bad to 0
    set temp to ( in1 + in4 )
    if ( temp > 1 )
    	set bad to 1
    endif
    set temp to ( in1 + in5 )
    if ( temp > 1 )
    	set bad to 1
    endif
    set temp to ( in4 + in5 )
    if ( temp > 1 )
    	set bad to 1
    endif
    if ( player->GetItemCount "A1V6_L30_invisibility" == 1 )
    	player->RemoveItem "A1V6_L30_invisibility", 1
    elseif ( player->GetItemCount "A1V6_L60_invisibility" == 1 )
    	player->RemoveItem "A1V6_L60_invisibility", 1
    endif
    if ( bad == 0 )
    	if ( sum30 > 1 )
    		player->AddItem "A1V6_L30_invisibility", 1
    	elseif ( sum60 > 1 )
    		player->AddItem "A1V6_L60_invisibility", 1
    	endif
    endif
    
    StopScript A1V6_ACheck35_Invis_sc
    StartScript A1V6_ACheck36_LightSh_sc
    
    End

