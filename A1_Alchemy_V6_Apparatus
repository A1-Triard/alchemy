    if ( player->GetItemCount p_invisibility_b_A1_8 >= 8 )
    	player->AddItem p_invisibility_b, 1
    	player->RemoveItem p_invisibility_b_A1_8, 8
    endif
    if ( player->GetItemCount p_invisibility_c_A1_8 >= 8 )
