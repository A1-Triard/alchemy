    elseif ( player->GetItemCount apparatus_m_mortar_01 > 0 )
    	player->RemoveItem apparatus_m_mortar_01, 1
    	AddItem apparatus_m_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_m_mortar_e2, 1, 50, 0
    elseif ( player->GetItemCount apparatus_j_mortar_01 > 0 )
    	player->RemoveItem apparatus_j_mortar_01, 1
    	AddItem apparatus_j_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_j_mortar_e2, 1, 50, 0
    elseif ( player->GetItemCount apparatus_a_mortar_01 > 0 )
    	player->RemoveItem apparatus_a_mortar_01, 1
    	AddItem apparatus_a_mortar_01, 1
    	PlaceAtPC A1V6_apparatus_a_mortar_e2, 1, 50, 0
    endif
    
    MessageBox "Установка готова"
    EnablePlayerControls
    
    StopScript A1V6_ApparatusSetUp_2
    
    End

