SCHD A1V6_AAdd10_DetKey_sc 9 0 0 744 53
SCVR in1;in2;in3;in4;sum;temp;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd10_DetKey_sc
    
    short in1
    short in2
    short in3
    short in4
    short sum
    short temp
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck10_DetKey_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy5_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set in1 to 0
    set in2 to 0
    set in3 to 0
    set in4 to 0
    
    if ( player->GetItemCount "ingred_ash_yam_01" > 0 )
    	set in1 to 1
    endif
    if ( player->GetItemCount "ingred_bc_spore_pod" > 0 )
    	set in2 to 1
    endif
    if ( player->GetItemCount "ingred_diamond_01" > 0 )
    	set in3 to 1
    endif
    if ( player->GetItemCount "ingred_muck_01" > 0 )
    	set in4 to 1
    endif
    
    set sum to ( in1 + in2 )
    set sum to ( sum + in3 )
    set sum to ( sum + in4 )
    if ( sum > 1 )
    	if ( in1 > 0 )
    		player->RemoveItem "ingred_ash_yam_01", 1
    	endif
    	if ( in2 > 0 )
    		player->RemoveItem "ingred_bc_spore_pod", 1
    	endif
    	if ( in3 > 0 )
    		player->RemoveItem "ingred_diamond_01", 1
    	endif
    	if ( in4 > 0 )
    		player->RemoveItem "ingred_muck_01", 1
    	endif
    	set state to 1
    	set A1V6_CalcAlchemy5_sc.in to sum
    	set A1V6_CalcAlchemy5 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

SCPT
