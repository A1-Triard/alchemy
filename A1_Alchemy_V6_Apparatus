SCHD A1V6_IncAlchemy 1 2 0 218 12
SCVR a;str;pcRef;;
SCDT BQFzAQAUIEcQQTFWNl9BbGNoZW15UGx1cwATOAAAEzhzAAA8EzgsAAo4QAATQTFWNl9BbGNoZW15UGx1c18lZAs/EzgAABM4bAACPBM4VAAKOFwABnBsYXllcgAdPBM4AQATOGwAAjwROAEAAAATOAAAEzhsAAA8EzgBABM4bAAAPBg8KDwTOAAAEzhsAAA8EzgBABM4bAAAPBg8Dj8FAXMBAAggcwEAIDEgKwUBRxBBMVY2X0FsY2hlbXlQbHVzBSBzAQAAHBAPQTFWNl9JbmNBbGNoZW15AQE=
SCTX
    Begin A1V6_IncAlchemy
    
    short a
    long str
    long pcRef
    
    set a to A1V6_AlchemyPlus
    setx str to xStringBuild "A1V6_AlchemyPlus_%d" a
    setx pcRef to xGetRef "player"
    pcRef->xAddItem str 1
    pcRef->xEquip str
    set a to ( a + 1 )
    set A1V6_AlchemyPlus to a
    
    StopScript A1V6_IncAlchemy
    
    End

SCPT
SCHD A1V6_ModAlchemyExp 0 0 5 370 32
SCVR Value;alch1;alch2;tempf1;tempf2;;
SCDT 
SCTX
    Begin A1V6_ModAlchemyExp
    
    float Value
    float alch1
    float alch2
    float tempf1
    float tempf2
    
    set tempf1 to ( 1.5 * Value )
    set tempf2 to A1V6_AlchemyExp
    set tempf1 to ( tempf1 + tempf2 )
    set A1V6_AlchemyExp to tempf1
    if ( A1V6_AlchemyExp >= 100 )
    	set A1V6_AlchemyExp to 0
    	if ( Gals__CharDev_Present == 0 )
    		if ( A1V6_AlchemyPlus < 100 )
    			StartScript A1V6_IncAlchemy
    		endif
    	else
    		set alch1 to ( player->GetAlchemy )
    		player->ModAlchemy 1
    		set alch2 to ( player->GetAlchemy )
    		if ( alch2 > alch1 )
    			PlaySound skillraise
    			MessageBox "Ваш навык алхимии вырос до %.0f", alch2
    		endif
    	endif
    endif
    
    StopScript A1V6_ModAlchemyExp
    
    End

SCPT
SCHD A1V6_Mortar_1_sc 1 0 3 224 16
SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V6_Mortar_1_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V6_ApparatusDisable_1 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V6_MortarX
    	set cy to A1V6_MortarY
    	set cz to A1V6_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

SCPT
SCHD A1V6_Mortar_2_sc 1 0 3 224 16
SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V6_Mortar_2_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V6_ApparatusDisable_2 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V6_MortarX
    	set cy to A1V6_MortarY
    	set cz to A1V6_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

