NAME A1V4_CalcAlchemy2
FNAM s%
FLTV 0.0

SCPT
SCHD A1V4_AlchemyActivator_sc 2 0 0 56 22
SCVR PCSkipEquip;OnPCEquip;;
SCDT BQFzAQACIDEGAQIJIHMCACA9PSAxBQFzAgACIDAbEBRBMVY0X0FsY2hlbXlDaGVja19zYwkBAQE=
SCTX
    Begin A1V4_AlchemyActivator_sc
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( OnPCEquip == 1 )
    	set OnPCEquip to 0
    	StartScript A1V4_AlchemyCheck_sc
    endif
    
    End

SCPT
SCHD A1V4_AlchemyCheck_sc 0 0 0 57 0
SCDT HBAUQTFWNF9BbGNoZW15Q2hlY2tfc2MbEB1BMVY0X0FDaGVjazFfUmVzdG9yZUhlYWx0aF9zYwEB
SCTX
    Begin A1V4_AlchemyCheck_sc
    
    StopScript A1V4_AlchemyCheck_sc
    StartScript A1V4_ACheck1_RestoreHealth_sc
    
    End

SCPT
SCHD A1V4_Alembic_1_sc 1 0 3 226 16
SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V4_Alembic_1_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V4_ApparatusDisable_1 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V4_AlembicX
    	set cy to A1V4_AlembicY
    	set cz to A1V4_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

SCPT
SCHD A1V4_Alembic_2_sc 1 0 3 226 16
SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V4_Alembic_2_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V4_ApparatusDisable_2 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V4_AlembicX
    	set cy to A1V4_AlembicY
    	set cz to A1V4_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

