    elseif ( player->GetItemCount "A1V4_L60_restore_strength" == 1 )
    	player->RemoveItem "A1V4_L60_restore_strength", 1
    endif
    
    StopScript A1V4_ADel45_RestoreStr_sc
    StartScript A1V4_ADel46_RestoreWill_sc
    
    End

