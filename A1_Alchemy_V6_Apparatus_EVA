SCVR in;add;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd12_Feather_60
    
    short in
    short add
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck12_Feather_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
