    	elseif ( in == 6 )
    		player->RemoveItem "ingred_chokeweed_01", 1
    	else
    		player->RemoveItem "ingred_daedra_skin_01", 1
    	endif
    endif
    if ( add == 1 )
    	set state to 1
    	set A1V6_CalcAlchemy2_sc.pin to 40
    	set A1V6_CalcAlchemy2_sc.in to 2
    	set A1V6_CalcAlchemy2 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

