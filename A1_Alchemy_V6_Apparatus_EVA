    		player->AddItem p_cure_common_s_A1_16, 4
    	elseif ( A1V6_PotionS == 5 )
    		player->AddItem p_cure_common_s_A1_16, 5
    	elseif ( A1V6_PotionS == 6 )
    		player->AddItem p_cure_common_s_A1_16, 6
    	elseif ( A1V6_PotionS == 7 )
    		player->AddItem p_cure_common_s_A1_16, 7
    	elseif ( A1V6_PotionS == 8 )
    		player->AddItem p_cure_common_s_A1_16, 8
    	endif
    	if ( A1V6_PotionQ == 1 )
    		player->AddItem p_cure_common_s_A1_16, 1
    	elseif ( A1V6_PotionQ == 2 )
    		player->AddItem p_cure_common_s_A1_16, 2
    	elseif ( A1V6_PotionQ == 3 )
    		player->AddItem p_cure_common_s_A1_16, 3
    	elseif ( A1V6_PotionQ == 4 )
    		player->AddItem p_cure_common_s_A1_16, 4
    	elseif ( A1V6_PotionQ == 5 )
    		player->AddItem p_cure_common_s_A1_16, 5
    	elseif ( A1V6_PotionQ == 6 )
    		player->AddItem p_cure_common_s_A1_16, 6
    	elseif ( A1V6_PotionQ == 7 )
    		player->AddItem p_cure_common_s_A1_16, 7
    	elseif ( A1V6_PotionQ == 8 )
    		player->AddItem p_cure_common_s_A1_16, 8
    	endif
    endif
    
    set AddPotion to 0
    
    if ( player->GetItemCount p_cure_common_s_A1_16 >= 16 )
    	player->AddItem p_cure_common_s, 1
    	player->RemoveItem p_cure_common_s_A1_16, 16
    endif
    if ( player->GetItemCount p_cure_common_s_A1_16 >= 16 )
    	player->AddItem p_cure_common_s, 1
    	player->RemoveItem p_cure_common_s_A1_16, 16
    endif
    if ( player->GetItemCount p_cure_common_s_A1_16 >= 16 )
    	player->AddItem p_cure_common_s, 1
    	player->RemoveItem p_cure_common_s_A1_16, 16
    endif
    if ( player->GetItemCount p_cure_common_s_A1_16 >= 16 )
    	player->AddItem p_cure_common_s, 1
    	player->RemoveItem p_cure_common_s_A1_16, 16
    endif
    
    if ( player->GetItemCount "A1V6_L30_cure_common" == 1 )
    	player->RemoveItem "A1V6_L30_cure_common", 1
    elseif ( player->GetItemCount "A1V6_L45_cure_common" == 1 )
    	player->RemoveItem "A1V6_L45_cure_common", 1
    elseif ( player->GetItemCount "A1V6_L60_cure_common" == 1 )
    	player->RemoveItem "A1V6_L60_cure_common", 1
    endif
    
    set in30 to 0
    set in45 to 0
    set in60 to 0
    
    if ( player->GetItemCount "ingred_gravedust_01" > 0 )
    	set in30 to 1
    elseif ( player->GetItemCount "ingred_red_lichen_01" > 0 )
    	set in45 to 1
    elseif ( player->GetItemCount "ingred_blood_innocent_unique" > 0 )
    	set in45 to ( in45 + 1 )
    endif
    if ( player->GetItemCount "ingred_willow_anther_01" > 0 )
    	set in45 to ( in45 + 1 )
    elseif ( player->GetItemCount "ingred_muck_01" > 0 )
    	set in60 to 1
    endif
    if ( player->GetItemCount "ingred_chokeweed_01" > 0 )
    	set in45 to ( in45 + 1 )
    endif
    if ( player->GetItemCount "ingred_daedra_skin_01" > 0 )
    	set in30 to ( in30 + 1 )
    endif
    if ( player->GetItemCount "ingred_green_lichen_01" > 0 )
    	set in30 to ( in30 + 1 )
    endif
    
    set in45 to ( in45 + in30 )
    set in60 to ( in60 + in45 )
    
    if ( in30 > 1 )
    	player->AddItem "A1V6_L30_cure_common", 1
    elseif ( in45 > 1 )
    	player->AddItem "A1V6_L45_cure_common", 1
    elseif ( in60 > 1 )
    	player->AddItem "A1V6_L60_cure_common", 1
    endif
    
    StopScript A1V6_ACheck9_Cure_sc
    StartScript A1V6_ACheck10_DetKey_sc
    
    End

