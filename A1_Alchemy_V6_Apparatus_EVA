SCVR doOnce;cx;cy;cz;;
SCDT 
SCTX
    Begin A1V6_Calcinator_1_sc
    
    short doOnce
    float cx
    float cy
    float cz
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( A1V6_ApparatusDisable_1 == 1 )
    	Disable
    endif
    
    if ( OnActivate == 1 )
    	return
    endif
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	set cx to A1V6_CalcinatorX
    	set cy to A1V6_CalcinatorY
    	set cz to A1V6_ApparatusZ
    	set cz to ( cz - 15 )
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    endif
    
    End

