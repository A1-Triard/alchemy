SCHD A1V6_ApparatusItem_1_sc 2 0 0 513 17
SCVR OnPCAdd;OnPCDrop;;
SCDT 
SCTX
    Begin A1V6_ApparatusItem_1_sc
    
    short OnPCAdd
    short OnPCDrop
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( OnPCAdd == 1 )
    	set OnPCAdd to 0
    	if ( player->GetItemCount A1V6_ApparatusItem_2 == 1 )
    		player->RemoveItem A1V6_ApparatusItem_2, 1
    	endif
    	if ( player->GetItemCount A1V6_ApparatusItem_3 == 1 )
    		player->RemoveItem A1V6_ApparatusItem_3, 1
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set OnPCDrop to 0
    	set A1V6_ApparatusX to GetPos X
    	set A1V6_ApparatusY to GetPos Y
    	set A1V6_ApparatusZ to GetPos Z
    	set A1V6_ApparatusAngle to ( player -> GetAngle Z )
    	PlaceAtPC "A1V6_ApparatusCont_1" 1, 50, 0
    	if ( A1V6_ApparatusExt_2 == 0 )
    		player->AddItem A1V6_ApparatusItem_2, 1
    	elseif ( A1V6_ApparatusExt_3 == 0 )
    		player->AddItem A1V6_ApparatusItem_3, 1
    	endif
    	Disable
    endif
    
    End

