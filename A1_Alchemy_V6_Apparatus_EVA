SCHD A1V6_AAdd11_Poison_60 5 0 0 1908 35
SCVR in;add;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd11_Poison_60
    
    short in
    short add
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck11_Poison_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy2_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set add to 0
    set in to 0
    
    if ( player->GetItemCount "ingred_scathecraw_01" > 0 )
    	set in to 1
    elseif ( player->GetItemCount "ingred_roobrush_01" > 0 )
    	set in to 2
    elseif ( player->GetItemCount "ingred_black_lichen_01" > 0 )
    	set in to 3
    endif
    if ( player->GetItemCount "ingred_scrib_jelly_02" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_scrib_jelly_02", 1
    	else
    		set in to 4
    	endif
    elseif ( player->GetItemCount "ingred_scrib_jelly_01" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_scrib_jelly_01", 1
    	else
    		set in to 5
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_ghoul_heart_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_ghoul_heart_01", 1
    		else
    			set in to 6
    		endif
    	elseif ( player->GetItemCount "ingred_rat_meat_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_rat_meat_01", 1
    		else
    			set in to 7
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "al_i_rat_eye" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "al_i_rat_eye", 1
    		else
    			set in to 8
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_raw_ebony_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_raw_ebony_01", 1
    		else
    			set in to 9
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( in > 0 )
    		if ( player->GetItemCount "al_i_shalk_mandible" > 0 )
    			set add to 1
    			player->RemoveItem "al_i_shalk_mandible", 1
    		endif
    	endif
    endif
    
    if ( add == 1 )
    	if ( in == 1 )
    		player->RemoveItem "ingred_scathecraw_01", 1
    	elseif ( in == 2 )
    		player->RemoveItem "ingred_roobrush_01", 1
    	elseif ( in == 3 )
    		player->RemoveItem "ingred_black_lichen_01", 1
    	elseif ( in == 4 )
    		player->RemoveItem "ingred_scrib_jelly_02", 1
    	elseif ( in == 5 )
    		player->RemoveItem "ingred_scrib_jelly_01", 1
    	elseif ( in == 6 )
    		player->RemoveItem "ingred_ghoul_heart_01", 1
    	elseif ( in == 7 )
    		player->RemoveItem "ingred_rat_meat_01", 1
    	elseif ( in == 8 )
    		player->RemoveItem "al_i_rat_eye", 1
    	else
    		player->RemoveItem "ingred_raw_ebony_01", 1
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_roobrush_01" > 0 )
    		if ( player->GetItemCount "ingred_black_lichen_01" > 0 )
    			player->RemoveItem "ingred_roobrush_01", 1
    			player->RemoveItem "ingred_black_lichen_01", 1
    			set add to 1
    		endif
    	endif
    endif
    if ( add == 1 )
    	set state to 1
    	set A1V6_CalcAlchemy2_sc.pin to 40
    	set A1V6_CalcAlchemy2_sc.in to 2
    	set A1V6_CalcAlchemy2 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

