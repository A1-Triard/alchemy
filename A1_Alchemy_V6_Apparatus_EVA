    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck12_Feather_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy5_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
