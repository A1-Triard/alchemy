SCPT
SCHD A1V6_AAdd10_DetKey_45 5 0 0 653 35
SCVR in;add;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd10_DetKey_45
    
    short in
    short add
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck10_DetKey_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy5_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set add to 0
    set in to 0
    
    if ( player->GetItemCount "ingred_bc_spore_pod" > 0 )
    	set in to 1
    endif
    if ( player->GetItemCount "ingred_muck_01" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_muck_01", 1
    	else
    		set in to 2
    	endif
    endif
    if ( add == 0 )
    	if ( in > 0 )
    		if ( player->GetItemCount "al_i_ogrim_teeth" > 0 )
    			set add to 1
    			player->RemoveItem "al_i_ogrim_teeth", 1
    		endif
    	endif
    endif
    
    if ( add == 1 )
    	if ( in == 1 )
    		player->RemoveItem "ingred_bc_spore_pod", 1
    	else
    		player->RemoveItem "ingred_muck_01", 1
    	endif
    endif
    if ( add == 1 )
    	set state to 1
    	set A1V6_CalcAlchemy5_sc.in to 2
    	set A1V6_CalcAlchemy5 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

SCPT
SCHD A1V6_AAdd10_DetKey_60 5 0 0 1076 35
SCVR in;add;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd10_DetKey_60
    
    short in
    short add
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck10_DetKey_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy5_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set add to 0
    set in to 0
    
    if ( player->GetItemCount "ingred_bc_spore_pod" > 0 )
    	set in to 1
    elseif ( player->GetItemCount "al_i_animal_skin" > 0 )
    	set in to 2
    endif
    if ( player->GetItemCount "ingred_diamond_01" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_diamond_01", 1
    	else
    		set in to 3
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_ash_yam_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_ash_yam_01", 1
    		else
    			set in to 4
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_muck_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_muck_01", 1
    		else
    			set in to 5
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( in > 0 )
    		if ( player->GetItemCount "al_i_ogrim_teeth" > 0 )
    			set add to 1
    			player->RemoveItem "al_i_ogrim_teeth", 1
    		endif
    	endif
    endif
    
    if ( add == 1 )
    	if ( in == 1 )
    		player->RemoveItem "ingred_bc_spore_pod", 1
    	elseif ( in == 2 )
    		player->RemoveItem "al_i_animal_skin", 1
    	elseif ( in == 3 )
    		player->RemoveItem "ingred_diamond_01", 1
    	elseif ( in == 4 )
    		player->RemoveItem "ingred_ash_yam_01", 1
    	else
    		player->RemoveItem "ingred_muck_01", 1
    	endif
    endif
    if ( add == 1 )
    	set state to 1
    	set A1V6_CalcAlchemy5_sc.in to 2
    	set A1V6_CalcAlchemy5 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

SCPT
SCHD A1V6_AAdd11_Poison_30 5 0 0 1224 35
SCVR in;add;state;PCSkipEquip;OnPCEquip;;
SCDT 
SCTX
    Begin A1V6_AAdd11_Poison_30
    
    short in
    short add
    short state
    
    short PCSkipEquip
    short OnPCEquip
    
    set PCSkipEquip to 1
    
    if ( state == 2 )
    	set A1V6_ACheck11_Poison_sc.AddPotion to 1
    	StartScript A1V6_AlchemyCheck_sc
    	set state to 0
    	return
    endif
    
    if ( state == 1 )
    	if ( ScriptRunning A1V6_CalcAlchemy2_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set add to 0
    set in to 0
    
    if ( player->GetItemCount "ingred_scathecraw_01" > 0 )
    	set in to 1
    endif
    if ( player->GetItemCount "ingred_scrib_jelly_02" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_scrib_jelly_02", 1
    	else
    		set in to 2
    	endif
    elseif ( player->GetItemCount "ingred_scrib_jelly_01" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "ingred_scrib_jelly_01", 1
    	else
    		set in to 3
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_ghoul_heart_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_ghoul_heart_01", 1
    		else
    			set in to 4
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_raw_ebony_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_raw_ebony_01", 1
    		else
    			set in to 5
    		endif
    	endif
    endif
    if ( add == 0 )
    	if ( in > 0 )
    		if ( player->GetItemCount "al_i_shalk_mandible" > 0 )
    			set add to 1
    			player->RemoveItem "al_i_shalk_mandible", 1
    		endif
    	endif
    endif
    
    if ( add == 1 )
    	if ( in == 1 )
    		player->RemoveItem "ingred_scathecraw_01", 1
    	elseif ( in == 2 )
    		player->RemoveItem "ingred_scrib_jelly_02", 1
    	elseif ( in == 3 )
    		player->RemoveItem "ingred_scrib_jelly_01", 1
    	elseif ( in == 4 )
    		player->RemoveItem "ingred_ghoul_heart_01", 1
    	else
    		player->RemoveItem "ingred_raw_ebony_01", 1
    	endif
    endif
    if ( add == 1 )
    	set state to 1
    	set A1V6_CalcAlchemy2_sc.pin to 40
    	set A1V6_CalcAlchemy2_sc.in to 2
    	set A1V6_CalcAlchemy2 to 1
    else
    	StartScript A1V6_AlchemyCheck_sc
    endif
    
    endif
    
    End

SCPT
