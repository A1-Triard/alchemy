    	if ( ScriptRunning A1V6_CalcAlchemy5_sc == 0 )
    		set state to 2
    	endif
    	return
    endif
    
    if ( OnPCEquip == 1 )
    set OnPCEquip to 0
    
    set add to 0
    set in to 0
    
    if ( player->GetItemCount "ingred_ruby_01" > 0 )
    	set in to 1
    endif
    if ( player->GetItemCount "al_i_clannfear_skin" > 0 )
    	if ( in > 0 )
    		set add to 1
    		player->RemoveItem "al_i_clannfear_skin", 1
    	else
    		set in to 2
    	endif
    endif
    if ( add == 0 )
    	if ( player->GetItemCount "ingred_heather_01" > 0 )
    		if ( in > 0 )
    			set add to 1
    			player->RemoveItem "ingred_heather_01", 1
    		else
    			set in to 3
    		endif
    	endif
    endif
