3SET
VERSION 1067869798
TYPE ESP
AUTHOR A1
DESCRIPTION
    Дорожный мешок, позволяющий быстро выложить набор предметов.
    
    Команда для получения:
    player->AddItem A1V4_RoadBag$Item_#, 1
    Здесь # - номер мешка (1, 2, 3), $ - тип мешка (Ingr - ингредиенты, Misc - еда и т. п., Alch - зелья, Incp - неполные зелья).
Morrowind.esm 79764287
Tribunal.esm 6616539
Bloodmoon.esm 11799703

GLOB
NAME A1V4_RoadBagX
FNAM f%
FLTV AAAAAA==

GLOB
NAME A1V4_RoadBagY
FNAM f%
FLTV AAAAAA==

GLOB
NAME A1V4_RoadBagZ
FNAM f%
FLTV AAAAAA==

GLOB
NAME A1V4_RoadBagAngle
FNAM f%
FLTV AAAAAA==

SCPT
SCHD QTFWNF9Sb2FkQmFnQWxjaENvbnRfMV9zYwAAAAAAAAAHAAAACAAAAAUAAAAVBQAAmAAAAA==
SCVR
    doOnce
    doRemove
    doDisable
    temp
    temp2
    doActivate
    waitActivate
    invItem
    invCount
    invRef
    pcRef
    invRefType
    tempLong
    invName
    invNextRef
    cx
    cy
    cz
    ca
    invWeight
    
SCDT 
SCTX
    Begin A1V4_RoadBagAlchCont_1_sc
    
    short doOnce
    short doRemove
    short doDisable
    short temp
    short temp2
    float cx
    float cy
    float cz
    float ca
    long invItem
    long invCount
    long invRef
    long pcRef
    long invRefType
    long tempLong
    float invWeight
    long invName
    long invNextRef
    short doActivate
    short waitActivate
    
    if ( doDisable > 20 )
    	if ( GetDisabled == 1 )
    		SetDelete 1
    		return
    	endif
    endif
    
    if ( doDisable > 20 )
    	Disable
    	return
    endif
    
    if ( doDisable > 0 )
    	AddItem "A1V4_RoadBagFakeIngred", 2
    	set doDisable to ( doDisable + 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	Enable
    	set doDisable to 1
    endif
    
    if ( waitActivate == 2 )
    	if ( MenuMode == 0 )
    		set waitActivate to 0
    		if ( GetItemCount A1V4_RoadBagAlchItem_1 == 0 )
    			set doDisable to 1
    		endif
    	endif
    	return
    elseif ( waitActivate == 1 )
    	if ( MenuMode == 1 )
    		set waitActivate to 2
    	endif
    	return
    endif
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ifx ( doOnce )
    else
    	DisablePlayerControls
    	set doOnce to 1
    	set cx to A1V4_RoadBagX
    	set cy to A1V4_RoadBagY
    	set cz to A1V4_RoadBagZ
    	set ca to A1V4_RoadBagAngle
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    	SetAngle Z ca
    	setx invItem,invCount,invRef to xInventory
    	whilex ( invCount )
    		xRemoveItem invItem invCount
    		setx invItem,invCount,invRef to xInventory
    	endwhile
    	AddItem "A1V4_RoadBagAlchItem_1", 1
    	set invRef to 0
    	set doRemove to 1
    	return
    endif
    
    if ( doActivate == 1 )
    	set waitActivate to 1
    	set doActivate to 0
    	Activate
    	return
    endif
    
    if ( doRemove == 10 )
    	set doRemove to 0
    	MessageBox "Зелья выложены"
    	EnablePlayerControls
    	return
    endif
    
    if ( doRemove > 5 )
    	set temp to 1
    else
    	set temp to 0
    endif
    
    ifx ( temp )
    	setx pcRef to xGetRef "player"
    	whilex ( pcRef )
    		setx invItem,invCount,invRefType,tempLong,invWeight,invName,invNextRef to pcRef->xContentList invRef
    		ifx ( invItem )
    			if ( invRefType == 1212369985 )
    				set temp to 1
    			else
    				set temp to 0
    			endif
    			ifx ( temp )
    				setx tempLong to xStringCompare invName, "1/"
    				if ( tempLong > 0 )
    					set temp2 to 1
    				else
    					set temp2 to 0
    				endif
    				ifx ( temp2 )
    					setx tempLong to xStringCompare invName, "1/a"
    					if ( tempLong < 0 )
    						set temp to 0
    					endif
    				endif
    			endif
    			ifx ( temp )
    				pcRef->xRemoveItem invItem invCount
    				xAddItem invItem invCount
    				set invRef to 0
    				set pcRef to 0
    				set doRemove to 1
    			endif
    		endif
    		if ( doRemove > 1 )
    			set invRef to invNextRef
    		endif
    		if ( invNextRef == 0 )
    			set pcRef to 0
    			set doRemove to 10
    		endif
    	endwhile
    	return
    endif
    
    if ( doRemove > 0 )
    	set doRemove to ( doRemove + 1 )
    	return
    endif
    
    if ( OnActivate == 1 )
    	set doActivate to 1
    	return
    endif
    
    End

SCPT
