    OnPCDrop
    
SCDT BgECCSBY3BAgPT0gMasRAQAkAQkBBgEHCSBzAQAgPT0gMQUBcwEAAiAwBQFHDUExVjRfUm9hZEJhZ1gGIFgKEFgABQFHDUExVjRfUm9hZEJhZ1kGIFgKEFkABQFHDUExVjRfUm9hZEJhZ1oGIFgKEFoABQFHEUExVjRfUm9hZEJhZ0FuZ2xlBiBYDBBaAOYQFkExVjRfUm9hZEJhZ0FsY2hDb250XzEBAAAASEIAANsQCQEBAQ==
SCTX
    Begin A1V4_RoadBagAlchItem_1_sc
    
    short OnPCDrop
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	set OnPCDrop to 0
    	set A1V4_RoadBagX to GetPos X
    	set A1V4_RoadBagY to GetPos Y
    	set A1V4_RoadBagZ to GetPos Z
    	set A1V4_RoadBagAngle to GetAngle Z
    	PlaceAtPC A1V4_RoadBagAlchCont_1 1, 50, 0
    	Disable
    endif
    
    End

