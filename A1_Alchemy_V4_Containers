    endif
    
    if ( GetDisabled == 1 )
    	Enable
    	set doDisable to 1
    endif
    
    if ( waitActivate == 2 )
    	if ( MenuMode == 0 )
    		set waitActivate to 0
    		if ( GetItemCount A1V4_RoadBagMiscItem_2 == 0 )
    			set doDisable to 1
    		endif
    	endif
    	return
    elseif ( waitActivate == 1 )
    	if ( MenuMode == 1 )
    		set waitActivate to 2
    	endif
    	return
    endif
    
